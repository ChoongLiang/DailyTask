{
    "name": "Daily Task",
    "version": "1.0.1",
    "description": "Time management quadrants",
    "permissions": ["activeTab", "storage"],
    "icons": {
      "16": "images/DailyTask_logo_16.png",
      "32": "images/DailyTask_logo_32.png",
      "48": "images/DailyTask_logo_48.png",
      "128": "images/DailyTask_logo_128.png"
    },
    "chrome_url_overrides" : { "newtab": "dailytask.html" },
    "background": {
      "scripts": ["js/jquery.min.js", "js/jquery-ui.js", "js/background.js",
                  "js/drag.js", "js/user_storage.js", "js/transition.js"]
    },
    "browser_action": {
      "default_icon": "images/DailyTask_logo_32.png"
    },
    "manifest_version": 2
 }