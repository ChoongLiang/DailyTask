{
    "name": "Daily Task",
    "version": "1.0",
    "description": "Seek to improve user time management. Idea credits to '7 Habits for Highly Effective People'",
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "chrome_url_overrides" : {
    "newtab": "dailytask.html"
  	},
    "dailytask": {
      "page": "dailytask.html",
      "scripts": ["js/transition.js", "js/jquery.min.js"]
    },
    "manifest_version": 2
 }